{
  "name": "@xyo-network/sdk-xyo-mongo-js",
  "version": "2.1.21",
  "homepage": "https://xyo.network",
  "bugs": {
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-mongo-js/issues",
    "email": "support@xyo.network"
  },
  "description": "Base functionality used throughout XYO TypeScript/JavaScript libraries that access Mongo DB",
  "author": {
    "name": "XYO Development Team",
    "url": "https://xyo.network",
    "email": "support@xyo.network"
  },
  "license": "LGPL-3.0",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-mongo-js.git"
  },
  "main": "dist/node/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "esm5": "dist/esm5/index.js",
  "exports": {
    ".": {
      "esm5": "./dist/esm5/index.js",
      "node": {
        "import": "./dist/node-esm/index.js",
        "require": "./dist/node/index.js"
      },
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "keywords": [
    "xyo",
    "utility",
    "typescript",
    "mongo"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@xylabs/sdk-js": "^2.3.19",
    "async-mutex": "^0.3.2",
    "mongodb": "^4.1.3"
  },
  "scripts": {
    "compile": "rm -r -f dist && yarn compile-rollup && yarn compile-tsc",
    "compile-rollup": "rollup -c",
    "compile-tsc": "tsc -p tsconfig.build.json",
    "lint-pkg": "npmPkgJsonLint .",
    "test": "jest"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-strip": "^2.1.0",
    "@types/bn.js": "^5.1.0",
    "@types/jest": "^27.0.2",
    "@xylabs/eslint-config": "2.1.6",
    "@xylabs/ts-scripts": "^1.0.15",
    "axios": "^0.24.0",
    "bn.js": "^5.2.0",
    "eslint": "^8.4.1",
    "jest": "^27.3.1",
    "npm-package-json-lint": "^5.4.1",
    "npm-package-json-lint-config-default": "^3.0.0",
    "prettier": "^2.5.1",
    "rollbar": "^2.24.0",
    "rollup": "^2",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-typescript2": "^0.31.1",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4"
  },
  "type": "module"
}