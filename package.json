{
  "name": "@xyo-network/sdk-xyo-mongo-js",
  "version": "2.2.2",
  "homepage": "https://xyo.network",
  "bugs": {
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-mongo-js/issues",
    "email": "support@xyo.network"
  },
  "description": "Base functionality used throughout XYO TypeScript/JavaScript libraries that access Mongo DB",
  "author": {
    "name": "XYO Development Team",
    "url": "https://xyo.network",
    "email": "support@xyo.network"
  },
  "license": "LGPL-3.0",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-mongo-js.git"
  },
  "main": "dist/node/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "esm5": "dist/esm5/index.js",
  "exports": {
    ".": {
      "esm5": "./dist/esm5/index.js",
      "node": {
        "import": "./dist/node-esm/index.js",
        "require": "./dist/node/index.js"
      },
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "keywords": [
    "xyo",
    "utility",
    "typescript",
    "mongo"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@xylabs/sdk-js": "^2.3.24",
    "async-mutex": "^0.3.2",
    "mongodb": "^4.3.0"
  },
  "scripts": {
    "compile": "rm -r -f dist && yarn compile-rollup && yarn compile-tsc",
    "compile-rollup": "rollup -c",
    "compile-tsc": "tsc -p tsconfig.json",
    "lint-pkg": "npmPkgJsonLint .",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-replace": "^3.0.1",
    "@rollup/plugin-strip": "^2.1.0",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/bn.js": "^5.1.0",
    "@types/jest": "^27.4.0",
    "@xylabs/eslint-config": "2.2.10",
    "@xylabs/rollup-config": "^1.0.5",
    "@xylabs/ts-scripts": "^1.0.24",
    "@xylabs/tsconfig": "^1.0.8",
    "axios": "^0.24.0",
    "babel-jest": "^27.4.6",
    "bn.js": "^5.2.0",
    "eslint": "^8.6.0",
    "jest": "^27.4.7",
    "npm-package-json-lint": "^5.4.2",
    "npm-package-json-lint-config-default": "^3.0.0",
    "prettier": "^2.5.1",
    "rollbar": "^2.24.0",
    "rollup": "^2",
    "ts-jest": "^27.1.2",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  },
  "type": "module"
}
