{
  "name": "@xyo-network/sdk-xyo-mongo-js",
  "version": "2.0.8",
  "homepage": "https://xyo.network",
  "bugs": {
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-mongo-js/issues",
    "email": "support@xyo.network"
  },
  "description": "Base functionality used throughout XYO TypeScript/JavaScript libraries that access Mongo DB",
  "author": {
    "name": "XYO Development Team",
    "url": "https://xyo.network",
    "email": "support@xyo.network"
  },
  "license": "LGPL-3.0",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-mongo-js.git"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.js",
  "sideEffects": false,
  "dependencies": {
    "@xyo-network/sdk-xyo-js": "^2.0.12",
    "async-mutex": "^0.3.1",
    "mongodb": "^3.6.5"
  },
  "scripts": {
    "build": "yarn deps && yarn lint && yarn compile",
    "compile": "rollup -c rollup.config.ts",
    "deploy": "yarn version --patch && yarn publish --access public",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src",
    "fix": "eslint --ext .js,.jsx,.ts,.tsx ./src --fix",
    "clean": "rm -f *.tsbuildinfo && rm -r -f ./build",
    "reinstall": "rm -r -f ./node_modules && rm -f ./yarn.lock && rm -f ./yarn-error.log && yarn install && yarn outdated",
    "tsprune": "yarn ts-prune",
    "deps": "yarn depcheck --ignores \"vue\""
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/mongodb": "^3.6.12",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "depcheck": "^1.4.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sort-keys-fix": "^1.1.1",
    "eslint-plugin-typescript-sort-keys": "^1.6.0",
    "eslint-plugin-workspaces": "^0.6.2",
    "prettier": "^2.2.1",
    "rollup": "^2.42.4",
    "ts-prune": "^0.9.0",
    "tslib": "^2.2.0",
    "typescript": "~4.2.2",
    "vue": "^3.0.11"
  }
}
