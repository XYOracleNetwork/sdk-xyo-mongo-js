{
  "name": "@xyo-network/sdk-xyo-mongo-js",
  "version": "2.1.16",
  "homepage": "https://xyo.network",
  "bugs": {
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-mongo-js/issues",
    "email": "support@xyo.network"
  },
  "description": "Base functionality used throughout XYO TypeScript/JavaScript libraries that access Mongo DB",
  "author": {
    "name": "XYO Development Team",
    "url": "https://xyo.network",
    "email": "support@xyo.network"
  },
  "license": "LGPL-3.0",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-mongo-js.git"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "xyo",
    "utility",
    "typescript",
    "mongo"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@xyo-network/sdk-xyo-js": "^2.1.35",
    "async-mutex": "^0.3.1",
    "mongodb": "^4.1.0",
    "tslib": "^2.3.0"
  },
  "scripts": {
    "build": "rm -r -f dist && yarn deps && yarn lint && yarn lint-pkg && yarn compile",
    "compile": "tsc -p tsconfig.build.json && rollup -c rollup.config.ts",
    "deploy": "yarn version --patch && yarn build && yarn publish",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src",
    "lint-pkg": "npmPkgJsonLint .",
    "fix": "eslint --ext .js,.jsx,.ts,.tsx ./src --fix",
    "test": "jest",
    "clean": "rm -f *.tsbuildinfo && rm -r -f ./dist",
    "reinstall": "rm -r -f ./node_modules && rm -f ./yarn.lock && rm -f ./yarn-error.log && yarn install && yarn outdated",
    "tsprune": "yarn ts-prune",
    "up": "yarn ncu",
    "updo": "yarn ncu -u && yarn install",
    "deps": "yarn depcheck"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.2.5",
    "@types/jest": "^26.0.24",
    "@xylabs/eslint-config": "2.0.16",
    "depcheck": "^1.4.2",
    "eslint": "^7.32.0",
    "jest": "^27.0.6",
    "npm-check-updates": "^11.8.3",
    "npm-package-json-lint": "^5.2.2",
    "npm-package-json-lint-config-default": "^3.0.0",
    "rollup": "^2.55.1",
    "ts-jest": "^27.0.4",
    "ts-prune": "^0.9.2",
    "typescript": "^4.3.5",
    "vue": "^3.1.5"
  }
}