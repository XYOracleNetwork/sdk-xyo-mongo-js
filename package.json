{
  "name": "@xyo-network/sdk-xyo-mongo-js",
  "version": "2.1.16",
  "homepage": "https://xyo.network",
  "bugs": {
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-mongo-js/issues",
    "email": "support@xyo.network"
  },
  "description": "Base functionality used throughout XYO TypeScript/JavaScript libraries that access Mongo DB",
  "author": {
    "name": "XYO Development Team",
    "url": "https://xyo.network",
    "email": "support@xyo.network"
  },
  "license": "LGPL-3.0",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-mongo-js.git"
  },
  "main": "./index.cjs",
  "module": "./index.js",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "require": "./index.cjs",
      "default": "./index.js"
    },
    "./": {
      "require": "./index.cjs",
      "default": "./index.js"
    }
  },
  "keywords": [
    "xyo",
    "utility",
    "typescript",
    "mongo"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@xyo-network/sdk-xyo-js": "^2.1.15",
    "async-mutex": "^0.3.1",
    "mongodb": "^4.0.1",
    "tslib": "^2.2.0"
  },
  "scripts": {
    "build": "rm -r -f dist && yarn deps && yarn lint && yarn lint-pkg && yarn compile && yarn copy-src",
    "copy-src": "cp package.json dist && cp README.md dist && cp LICENSE dist && cp -R src/* dist",
    "compile": "tsc -p tsconfig.build.json && rollup -c rollup.config.ts",
    "deploy": "yarn version --patch && yarn build && yarn publish ./dist",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src",
    "lint-pkg": "npmPkgJsonLint .",
    "fix": "eslint --ext .js,.jsx,.ts,.tsx ./src --fix",
    "test": "jest",
    "clean": "rm -f *.tsbuildinfo && rm -r -f ./dist",
    "reinstall": "rm -r -f ./node_modules && rm -f ./yarn.lock && rm -f ./yarn-error.log && yarn install && yarn outdated",
    "tsprune": "yarn ts-prune",
    "deps": "yarn depcheck"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/jest": "^26.0.22",
    "@xylabs/eslint-config": "2.0.16",
    "depcheck": "^1.4.0",
    "eslint": "^7.24.0",
    "jest": "^27.0.1",
    "npm-package-json-lint": "^5.1.0",
    "npm-package-json-lint-config-default": "^3.0.0",
    "rollup": "^2.42.4",
    "ts-jest": "^27.0.0",
    "ts-prune": "^0.9.0",
    "typescript": "^4.3.2",
    "vue": "^3.1.5"
  }
}
