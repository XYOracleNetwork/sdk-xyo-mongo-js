{
  "name": "@xyo-network/sdk-xyo-mongo-js",
  "version": "2.2.8",
  "homepage": "https://xyo.network",
  "bugs": {
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-mongo-js/issues",
    "email": "support@xyo.network"
  },
  "description": "Base functionality used throughout XYO TypeScript/JavaScript libraries that access Mongo DB",
  "author": {
    "name": "XYO Development Team",
    "url": "https://xyo.network",
    "email": "support@xyo.network"
  },
  "license": "LGPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-mongo-js.git"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "browser": {
    "mongodb": false
  },
  "exports": {
    ".": {
      "node": {
        "import": "./dist/esm/index.js",
        "require": "./dist/cjs/index.js"
      },
      "default": "./dist/esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "keywords": [
    "xyo",
    "utility",
    "typescript",
    "mongo"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@xylabs/sdk-js": "^2.5.9",
    "async-mutex": "^0.3.2",
    "mongodb": "^4.8.0"
  },
  "scripts": {
    "deploy": "yarn version patch -i && yarn build && yarn npm publish",
    "build": "yarn build:esm && yarn build:cjs",
    "build:cjs": "tsc -p tsconfig.build.cjs.json",
    "build:esm": "tsc -p tsconfig.build.esm.json",
    "lint-pkg": "npmPkgJsonLint .",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@types/jest": "^28.1.5",
    "@xylabs/eslint-config": "2.3.30",
    "@xylabs/ts-scripts": "^1.0.66",
    "@xylabs/tsconfig": "^1.0.14",
    "axios": "^0.27.2",
    "babel-jest": "^28.1.3",
    "bn.js": "^5.2.1",
    "copyfiles": "^2.4.1",
    "eslint": "^8.19.0",
    "jest": "^28.1.3",
    "npm-package-json-lint": "^6.3.0",
    "npm-package-json-lint-config-default": "^5.0.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "rollbar": "^2.25.0",
    "ts-jest": "^28.0.6",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "packageManager": "yarn@3.1.1"
}
